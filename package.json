{
  "name": "@sprit/tailwindcss-webpack-plugin",
  "keywords": ["tailwindcss", "tailwindcss style override", "tailwindcss 样式覆盖", "tailwindcss 编译"],
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fantian007/tailwindcss-webpack-plugin"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "npx": "^10.2.2",
    "react": ">=16",
    "tailwindcss": "^3.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "赵永盛<1562743134@qq.com>",
  "module": "dist/tailwindcss-webpack-plugin.esm.js",
  "size-limit": [
    {
      "path": "dist/tailwindcss-webpack-plugin.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/tailwindcss-webpack-plugin.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "husky": "^8.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "safe-require": "^1.0.4",
    "size-limit": "^8.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "webpack": "^5.79.0"
  }
}